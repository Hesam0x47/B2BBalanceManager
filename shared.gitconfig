[pull]
    rebase = true

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    df = diff
    lg = log --graph --oneline --decorate
    docker-compose-dev="!docker compose -f docker-compose.yaml -f docker-compose.dev.yaml  --env-file .env.dev"
    docker-compose-prod="!docker compose -f docker-compose.yaml -f docker-compose.prod.yaml  --env-file .env"
